{
  "name": "@accounts/react",
  "version": "0.7.0",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "start": "tsc --watch",
    "precompile": "yarn clean",
    "compile": "tsc",
    "prepublishOnly": "yarn compile",
    "testonly": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "recompose": "^0.30.0"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@accounts/client": "^0.9.0",
    "@accounts/client-password": "^0.9.0",
    "@accounts/types": "^0.9.0",
    "@types/jest": "23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "jest": "23.6.0",
    "react": "^16.7.0"
  }
}
